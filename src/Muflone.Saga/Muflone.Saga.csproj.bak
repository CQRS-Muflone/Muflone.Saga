<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Authors>Alessandro Colla, Alberto Acerbis</Authors>
    <Copyright>Alessandro Colla, Alberto Acerbis and contributors</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Company>CQRS-Muflone</Company>
    <Description>Saga base implementation for Muflone</Description>
    <PackageProjectUrl>https://github.com/CQRS-Muflone/Muflone.Saga</PackageProjectUrl>
    <RepositoryUrl>https://github.com/CQRS-Muflone/Muflone.Saga</RepositoryUrl>
    <PackageIcon>Logo.png</PackageIcon>
    <NeutralLanguage>en</NeutralLanguage>
    <PackageTags>Muflone, Saga, CQRS, DDD</PackageTags>
    <AssemblyVersion></AssemblyVersion>
    <FileVersion></FileVersion>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <Version>2.0.0</Version>
    <SignAssembly>False</SignAssembly>
    <Title>$(AssemblyName)</Title>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\..\Logo.png">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
